//Principio de abierto-cerrado

class Mercadodespensa {
    mercados = ["Arroz", "Aceite", "Carne", "Jugo"];

    existemercado (mercado) {
        return this.mercados.indexOf(mercado) !== -1;
    }

    agregarmercado(mercado) {
        this.mercados.push(mercado);
    }
}
